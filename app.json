{
  "expo": {
    "name": "My Template",
    "slug": "mytemplate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "backgroundColor": "#000000",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lonewispapps.mytemplate"
    },
    "android": {
      "package": "com.lonewispapps.mytemplate",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "scheme": "myapp",
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "c5d64e25-310b-4879-ba47-0773ae003cce"
      },
      "router": {
        "origin": false
      }
    },
    "plugins": [
      "expo-router"
    ],
    "owner": "vittorjavidan"
  }
}
